.PHONY: all

TARGET = autogen

FLAGS = -std=c++14 -Wall -fPIC -O3 -DONNX_NAMESPACE=onnx -DONNX_ML=0 
INC = -I. -I${ONNX_PATH}/include -I$(PROTOBUF_PATH)/include
LINK = -L${ONNX_PATH}/lib  -L${PROTOBUF_PATH}/lib -lonnx -lonnx_proto -lprotobuf -lpthread 

all: $(TARGET)

$(TARGET): autogen.cpp
	g++ $(FLAGS) -o $@ $^ $(INC) $(LINK) 

clean:
	rm $(TARGET)
